{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "App/*": ["./App/*"],
      "store/*": ["./store/*"],
      "globalStyles/*": ["./globalStyles/*"],
      "components/*": ["./components/*"],
      "constants/*": ["./constants/*"],
      "assets/*": ["./assets/*"],
      "pages/*": ["./pages/*"],
      "layouts/*": ["./layouts/*"],
      "types/*": ["./types/*"],
      "utils/*": ["./utils/*"],
      "reducers/*": ["./reducers/*"],
      "selectors/*": ["./selectors/*"],
      "theme/*": ["./theme/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src"
  ]
}
